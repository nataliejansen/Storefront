<!DOCTYPE html>
<html lang="en">
<head>
    <title>Furniture Steals</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Mukta:300,400,700">
    <link rel="stylesheet" href="~/fonts/icomoon/style.css">

    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/magnific-popup.css">
    <link rel="stylesheet" href="~/css/jquery-ui.css">
    <link rel="stylesheet" href="~/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/css/owl.theme.default.min.css">

    <!-- Datatables-->
    <link href="//cdn.datatables.net/1.13.1/css/jquery.dataTables.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/css/aos.css">

    <link rel="stylesheet" href="~/css/style.css">



</head>
<body>

    <div class="site-wrap">
        <header class="site-navbar" role="banner">
            <div class="site-navbar-top">
                <div class="container">
                    <div class="row align-items-center">

                        <div class="col-6 col-md-4 order-2 order-md-1 site-search-icon text-left">
                            <form action="" class="site-block-top-search">
                                <span class="icon icon-search2"></span>
                                <input type="text" class="form-control border-0" placeholder="Search">
                            </form>
                        </div>

                        <div class="col-12 mb-3 mb-md-0 col-md-4 order-1 order-md-2 text-center">
                            <div class="site-logo">
                                <a asp-action="Index" asp-controller="Home" class="js-logo-clone">Furniture Steals</a>
                            </div>
                        </div>

                        <div class="col-6 col-md-4 order-3 order-md-3 text-right">
                            <div class="site-top-icons">
                                <ul>
                                    <li><a asp-action="ListUsers" asp-controller="Administration">
                                        <span class="icon icon-person"></span></a></li>
                                    <li><a href="#"><span class="icon icon-heart-o"></span></a></li>
                                    <li>
                                        <a asp-action="Index" asp-controller="ShoppingCart" class="site-cart">
                                            <span class="icon icon-shopping_cart"></span>
                                            <span class="count">2</span>
                                        </a>
                                    </li>
                                    <li class="d-inline-block d-md-none ml-md-0"><a href="#" class="site-menu-toggle js-menu-toggle"><span class="icon-menu"></span></a></li>
                                </ul>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <nav class="site-navigation text-right text-md-center" role="navigation">
                <div class="container">
                    <ul class="site-menu js-clone-nav d-none d-md-block">
                        <li><a asp-action="Index" asp-controller="Home">Home</a></li>
                        <li class="has-children active">
                            <a asp-action="About" asp-controller="Home">About</a>
                            <ul class="dropdown">
                                <li><a asp-action="About" asp-controller="Home">Our Story</a></li>
                                @*<li><a href="@Url.Action("TiledManufacturers", "Manufacturers")">Manufacturers</a></li>*@
                                <li><a asp-action="Policies" asp-controller="Home">Our Policies</a></li>

                            </ul>
                        </li>
                        <li class="has-children">
                            <a asp-action="Index" asp-controller="Products">Shop Products</a>
                            <ul class="dropdown">
                                <li><a href="#">Living Room</a></li>
                                <li><a href="#">Bedroom</a></li>
                                <li><a href="#">Dining</a></li>
                                <li><a href="#">Mattresses</a></li>
                                <li><a href="#">Rugs</a></li>
                                <li><a asp-action="Index" asp-controller="Products">All Products</a></li>
                            </ul>
                        </li>
                        <li class="has-children">
                            <a href="@Url.Action("ListUsers", "Administration")">Administration</a>
                            <ul class="dropdown">
                                <li><a href="@Url.Action("ListUsers", "Administration")">Users</a></li>
                                <li><a href="@Url.Action("ListRoles", "Administration")">Roles</a></li>
                                <li>
                                    <a href="@Url.Action("Index", "Manufacturers")">Manufacturers</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "Category")">Categories</a>
                                </li>
                            </ul>
                        </li>


                        <li><a asp-action="Contact" asp-controller="Home">Contact</a></li>
                    </ul>


                </div>
            </nav>
        </header>

        @RenderBody();

        <footer class="site-footer border-top">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 mb-5 mb-lg-0">
                        <div class="row">
                            <div class="col-md-12">
                                <h3 class="footer-heading mb-4">Navigations</h3>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <ul class="list-unstyled">
                                    <li><a asp-action="ListUsers" asp-controller="Administration">
                                        Administration List Users</a></li>
                                    <li><a href="#">Placeholder</a></li>
                                    <li><a href="#">Placeholder</a></li>

                                </ul>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <ul class="list-unstyled">
                                    <li><a href="#">Placeholder</a></li>
                                    <li><a href="#">Placeholder</a></li>
                                    <li><a href="#">Placeholder</a></li>
                                </ul>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <ul class="list-unstyled">
                                    <li><a href="#">Placeholder</a></li>
                                    <li><a href="#">Placeholder</a></li>
                                    <li><a href="#">Placeholder</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-3 mb-4 mb-lg-0">
                        <h3 class="footer-heading mb-4">Promo</h3>
                        <a href="#" class="block-6">
                            <img src="~/images/salephoto.jpg" alt="Image placeholder" class="img-fluid rounded mb-4">
                            <h3 class="mb-0">Act Fast!</h3>
                            <p>Sale ends 1/27/23</p>
                        </a>
                    </div>
                    <div class="col-md-6 col-lg-3">
                        <div class="block-5 mb-5">
                            <h3 class="footer-heading mb-4">Contact Info</h3>
                            <ul class="list-unstyled">
                                <li class="address">203 Fake St <br />Kansas City, Missouri, USA</li>
                                <li class="phone"><a href="tel://5555555555">+555-555-5555</a></li>
                                <li class="email">emailaddress@domain.com</li>
                                <a href="#" class="btn btn-sm btn-primary">Contact Form</a>
                            </ul>
                        </div>

                        <div class="block-7">
                            <form action="#" method="post">
                                <label for="email_subscribe" class="footer-heading">Subscribe</label>
                                <div class="form-group">
                                    <input type="text" class="form-control py-4" id="email_subscribe" placeholder="Email">
                                    <input type="submit" class="btn btn-sm btn-primary" value="Send">
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="row pt-5 mt-5 text-center">
                    <div class="col-md-12">
                        <p>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            Copyright &copy;<script data-cfasync="false" src="~/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>document.write(new Date().getFullYear());</script> All rights reserved | Natalie Kuechler
                        </p>
                    </div>

                </div>
            </div>
        </footer>
    </div>


    <script src="~/js/jquery-3.3.1.min.js"></script>
    <script src="~/js/jquery-ui.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/owl.carousel.min.js"></script>
    <script src="~/js/jquery.magnific-popup.min.js"></script>
    <script src="~/js/aos.js"></script>

    <!-- Datatables-->
    <script src="//cdn.datatables.net/1.13.1/js/jquery.dataTables.min.js"></script>

    <script src="~/js/main.js"></script>

    @RenderSection("scripts", false)

</body>
</html>